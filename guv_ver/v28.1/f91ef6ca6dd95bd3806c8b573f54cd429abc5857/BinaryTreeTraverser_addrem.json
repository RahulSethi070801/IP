        "28": "\n",
        "29": "/**\n",
        "30": " * A variant of {@link TreeTraverser} for binary trees, providing additional traversals specific to\n",
        "31": " * binary trees.\n",
        "32": " *\n",
        "33": " * @author Louis Wasserman\n",
        "34": " * @since 15.0\n",
        "35": " * @deprecated Use {@link com.google.common.graph.Traverser} instead. All instance methods except\n",
        "36": " *     for {@link #inOrderTraversal} have their equivalent on the result of {@code\n",
        "37": " *     Traverser.forTree(tree)} where {@code tree} implements {@code SuccessorsFunction}, which has\n",
        "38": " *     a similar API as {@link #children}.\n",
        "39": " *     <p>This class is scheduled to be removed in January 2018.\n",
        "40": " */\n",
        "41": "@Deprecated\n",
        "42": "@Beta\n",
        "43": "@GwtCompatible\n",
        "44": "public abstract class BinaryTreeTraverser<T> extends TreeTraverser<T> {\n",
        "45": "\n",
        "46": "  /**\n",
        "47": "   * Returns the left child of the specified node, or {@link Optional#absent()} if the specified\n",
        "48": "   * node has no left child.\n",
        "49": "   */\n",
        "50": "  public abstract Optional<T> leftChild(T root);\n",
        "51": "\n",
        "52": "  /**\n",
        "53": "   * Returns the right child of the specified node, or {@link Optional#absent()} if the specified\n",
        "54": "   * node has no right child.\n",
        "55": "   */\n",
        "56": "  public abstract Optional<T> rightChild(T root);\n",
        "57": "\n",
        "58": "  /** Returns the children of this node, in left-to-right order. */\n",
        "59": "  @Override\n",
        "60": "  public final Iterable<T> children(final T root) {\n",
        "61": "    checkNotNull(root);\n",
        "62": "    return new FluentIterable<T>() {\n",
        "63": "      @Override\n",
        "64": "      public Iterator<T> iterator() {\n",
        "65": "        return new AbstractIterator<T>() {\n",
        "66": "          boolean doneLeft;\n",
        "67": "          boolean doneRight;\n",
        "68": "\n",
        "69": "          @Override\n",
        "70": "          protected T computeNext() {\n",
        "71": "            if (!doneLeft) {\n",
        "72": "              doneLeft = true;\n",
        "73": "              Optional<T> left = leftChild(root);\n",
        "74": "              if (left.isPresent()) {\n",
        "75": "                return left.get();\n",
        "76": "              }\n",
        "77": "            }\n",
        "78": "            if (!doneRight) {\n",
        "79": "              doneRight = true;\n",
        "80": "              Optional<T> right = rightChild(root);\n",
        "81": "              if (right.isPresent()) {\n",
        "82": "                return right.get();\n",
        "83": "              }\n",
        "84": "            }\n",
        "85": "            return endOfData();\n",
        "86": "          }\n",
        "87": "        };\n",
        "88": "      }\n",
        "89": "    };\n",
        "90": "  }\n",
        "91": "\n",
        "92": "  @Override\n",
        "93": "  UnmodifiableIterator<T> preOrderIterator(T root) {\n",
        "94": "    return new PreOrderIterator(root);\n",
        "95": "  }\n",
        "96": "\n",
        "97": "  /*\n",
        "98": "   * Optimized implementation of preOrderIterator for binary trees.\n",
        "99": "   */\n",
        "100": "  private final class PreOrderIterator extends UnmodifiableIterator<T>\n",
        "101": "      implements PeekingIterator<T> {\n",
        "102": "    private final Deque<T> stack;\n",
        "103": "\n",
        "104": "    PreOrderIterator(T root) {\n",
        "105": "      this.stack = new ArrayDeque<T>(8);\n",
        "106": "      stack.addLast(root);\n",
        "107": "    }\n",
        "108": "\n",
        "109": "    @Override\n",
        "110": "    public boolean hasNext() {\n",
        "111": "      return !stack.isEmpty();\n",
        "112": "    }\n",
        "113": "\n",
        "114": "    @Override\n",
        "115": "    public T next() {\n",
        "116": "      T result = stack.removeLast();\n",
        "117": "      pushIfPresent(stack, rightChild(result));\n",
        "118": "      pushIfPresent(stack, leftChild(result));\n",
        "119": "      return result;\n",
        "120": "    }\n",
        "121": "\n",
        "122": "    @Override\n",
        "123": "    public T peek() {\n",
        "124": "      return stack.getLast();\n",
        "125": "    }\n",
        "126": "  }\n",
        "127": "\n",
        "128": "  @Override\n",
        "129": "  UnmodifiableIterator<T> postOrderIterator(T root) {\n",
        "130": "    return new PostOrderIterator(root);\n",
        "131": "  }\n",
        "132": "\n",
        "133": "  /*\n",
        "134": "   * Optimized implementation of postOrderIterator for binary trees.\n",
        "135": "   */\n",
        "136": "  private final class PostOrderIterator extends UnmodifiableIterator<T> {\n",
        "137": "    private final Deque<T> stack;\n",
        "138": "    private final BitSet hasExpanded;\n",
        "139": "\n",
        "140": "    PostOrderIterator(T root) {\n",
        "141": "      this.stack = new ArrayDeque<T>(8);\n",
        "142": "      stack.addLast(root);\n",
        "143": "      this.hasExpanded = new BitSet();\n",
        "144": "    }\n",
        "145": "\n",
        "146": "    @Override\n",
        "147": "    public boolean hasNext() {\n",
        "148": "      return !stack.isEmpty();\n",
        "149": "    }\n",
        "150": "\n",
        "151": "    @Override\n",
        "152": "    public T next() {\n",
        "153": "      while (true) {\n",
        "154": "        T node = stack.getLast();\n",
        "155": "        boolean expandedNode = hasExpanded.get(stack.size() - 1);\n",
        "156": "        if (expandedNode) {\n",
        "157": "          stack.removeLast();\n",
        "158": "          hasExpanded.clear(stack.size());\n",
        "159": "          return node;\n",
        "160": "        } else {\n",
        "161": "          hasExpanded.set(stack.size() - 1);\n",
        "162": "          pushIfPresent(stack, rightChild(node));\n",
        "163": "          pushIfPresent(stack, leftChild(node));\n",
        "164": "        }\n",
        "165": "      }\n",
        "166": "    }\n",
        "167": "  }\n",
        "168": "\n",
        "169": "  // TODO(lowasser): see if any significant optimizations are possible for breadthFirstIterator\n",
        "170": "\n",
        "171": "  public final FluentIterable<T> inOrderTraversal(final T root) {\n",
        "172": "    checkNotNull(root);\n",
        "173": "    return new FluentIterable<T>() {\n",
        "174": "      @Override\n",
        "175": "      public UnmodifiableIterator<T> iterator() {\n",
        "176": "        return new InOrderIterator(root);\n",
        "177": "      }\n",
        "178": "    };\n",
        "179": "  }\n",
        "180": "\n",
        "181": "  private final class InOrderIterator extends AbstractIterator<T> {\n",
        "182": "    private final Deque<T> stack;\n",
        "183": "    private final BitSet hasExpandedLeft;\n",
        "184": "\n",
        "185": "    InOrderIterator(T root) {\n",
        "186": "      this.stack = new ArrayDeque<T>(8);\n",
        "187": "      this.hasExpandedLeft = new BitSet();\n",
        "188": "      stack.addLast(root);\n",
        "189": "    }\n",
        "190": "\n",
        "191": "    @Override\n",
        "192": "    protected T computeNext() {\n",
        "193": "      while (!stack.isEmpty()) {\n",
        "194": "        T node = stack.getLast();\n",
        "195": "        if (hasExpandedLeft.get(stack.size() - 1)) {\n",
        "196": "          stack.removeLast();\n",
        "197": "          hasExpandedLeft.clear(stack.size());\n",
        "198": "          pushIfPresent(stack, rightChild(node));\n",
        "199": "          return node;\n",
        "200": "        } else {\n",
        "201": "          hasExpandedLeft.set(stack.size() - 1);\n",
        "202": "          pushIfPresent(stack, leftChild(node));\n",
        "203": "        }\n",
        "204": "      }\n",
        "205": "      return endOfData();\n",
        "206": "    }\n",
        "207": "  }\n",
        "208": "\n",
        "209": "  private static <T> void pushIfPresent(Deque<T> stack, Optional<T> node) {\n",
        "210": "    if (node.isPresent()) {\n",
        "211": "      stack.addLast(node.get());\n",
        "212": "    }\n",
        "213": "  }\n",
        "214": "}\n"
    }
