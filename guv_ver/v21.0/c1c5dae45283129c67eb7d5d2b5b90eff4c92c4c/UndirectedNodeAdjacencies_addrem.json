        "17": "package com.google.common.graph;\n",
        "18": "\n",
        "19": "import static com.google.common.base.Preconditions.checkNotNull;\n",
        "20": "\n",
        "21": "import com.google.common.base.MoreObjects;\n",
        "22": "import com.google.common.base.Objects;\n",
        "23": "import com.google.common.collect.ImmutableSet;\n",
        "24": "import com.google.common.collect.Sets;\n",
        "25": "\n",
        "26": "import java.util.Collections;\n",
        "27": "import java.util.Set;\n",
        "28": "\n",
        "29": "import javax.annotation.Nullable;\n",
        "30": "\n",
        "31": "/**\n",
        "32": " * A class representing an origin node's adjacent nodes in an undirected graph.\n",
        "33": " *\n",
        "34": " * @author James Sexton\n",
        "35": " * @param <N> Node parameter type\n",
        "36": " */\n",
        "37": "final class UndirectedNodeAdjacencies<N> implements NodeAdjacencies<N> {\n",
        "38": "  private final Set<N> adjacentNodes;\n",
        "39": "\n",
        "40": "  private UndirectedNodeAdjacencies(Set<N> adjacentNodes) {\n",
        "41": "    this.adjacentNodes = checkNotNull(adjacentNodes, \"adjacentNodes\");\n",
        "42": "  }\n",
        "43": "\n",
        "44": "  static <N> UndirectedNodeAdjacencies<N> of() {\n",
        "45": "    // TODO(user): Enable users to specify the expected number of neighbors of a new node.\n",
        "46": "    return new UndirectedNodeAdjacencies<N>(Sets.<N>newHashSet());\n",
        "47": "  }\n",
        "48": "\n",
        "49": "  static <N> UndirectedNodeAdjacencies<N> ofImmutable(Set<N> adjacentNodes) {\n",
        "50": "    return new UndirectedNodeAdjacencies<N>(ImmutableSet.copyOf(adjacentNodes));\n",
        "51": "  }\n",
        "52": "\n",
        "53": "  @Override\n",
        "54": "  public Set<N> adjacentNodes() {\n",
        "55": "    return Collections.unmodifiableSet(adjacentNodes);\n",
        "56": "  }\n",
        "57": "\n",
        "58": "  @Override\n",
        "59": "  public Set<N> predecessors() {\n",
        "60": "    return adjacentNodes();\n",
        "61": "  }\n",
        "62": "\n",
        "63": "  @Override\n",
        "64": "  public Set<N> successors() {\n",
        "65": "    return adjacentNodes();\n",
        "66": "  }\n",
        "67": "\n",
        "68": "  @Override\n",
        "69": "  public boolean removePredecessor(Object node) {\n",
        "70": "    return removeSuccessor(node);\n",
        "71": "  }\n",
        "72": "\n",
        "73": "  @Override\n",
        "74": "  public boolean removeSuccessor(Object node) {\n",
        "75": "    checkNotNull(node, \"node\");\n",
        "76": "    return adjacentNodes.remove(node);\n",
        "77": "  }\n",
        "78": "\n",
        "79": "  @Override\n",
        "80": "  public boolean addPredecessor(N node) {\n",
        "81": "    return addSuccessor(node);\n",
        "82": "  }\n",
        "83": "\n",
        "84": "  @Override\n",
        "85": "  public boolean addSuccessor(N node) {\n",
        "86": "    checkNotNull(node, \"node\");\n",
        "87": "    return adjacentNodes.add(node);\n",
        "88": "  }\n",
        "89": "\n",
        "90": "  @Override\n",
        "91": "  public int hashCode() {\n",
        "92": "    return Objects.hashCode(adjacentNodes);\n",
        "93": "  }\n",
        "94": "\n",
        "95": "  @Override\n",
        "96": "  public boolean equals(@Nullable Object object) {\n",
        "97": "    if (object instanceof UndirectedNodeAdjacencies) {\n",
        "98": "      UndirectedNodeAdjacencies<?> that = (UndirectedNodeAdjacencies<?>) object;\n",
        "99": "      return this.adjacentNodes.equals(that.adjacentNodes);\n",
        "100": "    }\n",
        "101": "    return false;\n",
        "102": "  }\n",
        "103": "\n",
        "104": "  @Override\n",
        "105": "  public String toString() {\n",
        "106": "    return MoreObjects.toStringHelper(this)\n",
        "107": "        .add(\"adjacentNodes\", adjacentNodes)\n",
        "108": "        .toString();\n",
        "109": "  }\n",
        "110": "}\n"
    },
    "removed": {}
}