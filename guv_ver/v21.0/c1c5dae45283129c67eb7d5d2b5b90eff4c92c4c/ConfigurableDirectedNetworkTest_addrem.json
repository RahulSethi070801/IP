        "17": "package com.google.common.graph;\n",
        "18": "\n",
        "19": "import static com.google.common.truth.Truth.assertThat;\n",
        "20": "import static org.junit.Assert.assertEquals;\n",
        "21": "import static org.junit.Assert.assertFalse;\n",
        "22": "import static org.junit.Assert.assertTrue;\n",
        "23": "import static org.junit.Assert.fail;\n",
        "24": "\n",
        "25": "import com.google.common.collect.ImmutableSet;\n",
        "26": "import com.google.common.testing.EqualsTester;\n",
        "27": "\n",
        "28": "import org.junit.Test;\n",
        "29": "import org.junit.runner.RunWith;\n",
        "30": "import org.junit.runners.JUnit4;\n",
        "31": "\n",
        "32": "/**\n",
        "33": " * Tests for a directed {@link ConfigurableNetwork} with default graph properties.\n",
        "34": " */\n",
        "35": "@RunWith(JUnit4.class)\n",
        "36": "public class ConfigurableDirectedNetworkTest extends ConfigurableSimpleDirectedNetworkTest {\n",
        "37": "\n",
        "38": "  @Override\n",
        "39": "  public MutableNetwork<Integer, String> createGraph() {\n",
        "40": "    return NetworkBuilder.directed().build();\n",
        "41": "  }\n",
        "42": "\n",
        "43": "  @Test\n",
        "44": "  public void edges_selfLoop() {\n",
        "45": "    addEdge(E11, N1, N1);\n",
        "46": "    assertThat(graph.edges()).containsExactly(E11);\n",
        "47": "  }\n",
        "48": "\n",
        "49": "  @Test\n",
        "50": "  public void incidentEdges_selfLoop() {\n",
        "51": "    addEdge(E11, N1, N1);\n",
        "52": "    assertThat(graph.incidentEdges(N1)).containsExactly(E11);\n",
        "53": "  }\n",
        "54": "\n",
        "55": "  @Test\n",
        "56": "  public void incidentNodes_selfLoop() {\n",
        "57": "    addEdge(E11, N1, N1);\n",
        "58": "    assertThat(graph.incidentNodes(E11)).containsExactly(N1);\n",
        "59": "  }\n",
        "60": "\n",
        "61": "  @Test\n",
        "62": "  public void adjacentNodes_selfLoop() {\n",
        "63": "    addEdge(E11, N1, N1);\n",
        "64": "    addEdge(E12, N1, N2);\n",
        "65": "    assertThat(graph.adjacentNodes(N1)).containsExactly(N1, N2);\n",
        "66": "  }\n",
        "67": "\n",
        "68": "  @Test\n",
        "69": "  public void adjacentEdges_selfLoop() {\n",
        "70": "    // An edge is never adjacent to itself\n",
        "71": "    addEdge(E11, N1, N1);\n",
        "72": "    assertThat(graph.adjacentEdges(E11)).isEmpty();\n",
        "73": "    addEdge(E12, N1, N2);\n",
        "74": "    assertThat(graph.adjacentEdges(E11)).containsExactly(E12);\n",
        "75": "  }\n",
        "76": "\n",
        "77": "  @Test\n",
        "78": "  public void edgesConnecting_selfLoop() {\n",
        "79": "    addEdge(E11, N1, N1);\n",
        "80": "    assertThat(graph.edgesConnecting(N1, N1)).containsExactly(E11);\n",
        "81": "    addEdge(E12, N1, N2);\n",
        "82": "    assertThat(graph.edgesConnecting(N1, N2)).containsExactly(E12);\n",
        "83": "    assertThat(graph.edgesConnecting(N1, N1)).containsExactly(E11);\n",
        "84": "  }\n",
        "85": "\n",
        "86": "  @Test\n",
        "87": "  public void inEdges_selfLoop() {\n",
        "88": "    addEdge(E11, N1, N1);\n",
        "89": "    assertThat(graph.inEdges(N1)).containsExactly(E11);\n",
        "90": "    addEdge(E41, N4, N1);\n",
        "91": "    assertThat(graph.inEdges(N1)).containsExactly(E11, E41);\n",
        "92": "  }\n",
        "93": "\n",
        "94": "  @Test\n",
        "95": "  public void outEdges_selfLoop() {\n",
        "96": "    addEdge(E11, N1, N1);\n",
        "97": "    assertThat(graph.outEdges(N1)).containsExactly(E11);\n",
        "98": "    addEdge(E12, N1, N2);\n",
        "99": "    assertThat(graph.outEdges(N1)).containsExactly(E11, E12);\n",
        "100": "  }\n",
        "101": "\n",
        "102": "  @Test\n",
        "103": "  public void predecessors_selfLoop() {\n",
        "104": "    addEdge(E11, N1, N1);\n",
        "105": "    assertThat(graph.predecessors(N1)).containsExactly(N1);\n",
        "106": "    addEdge(E41, N4, N1);\n",
        "107": "    assertThat(graph.predecessors(N1)).containsExactly(N1, N4);\n",
        "108": "  }\n",
        "109": "\n",
        "110": "  @Test\n",
        "111": "  public void successors_selfLoop() {\n",
        "112": "    addEdge(E11, N1, N1);\n",
        "113": "    assertThat(graph.successors(N1)).containsExactly(N1);\n",
        "114": "    addEdge(E12, N1, N2);\n",
        "115": "    assertThat(graph.successors(N1)).containsExactly(N1, N2);\n",
        "116": "  }\n",
        "117": "\n",
        "118": "  @Test\n",
        "119": "  public void degree_selfLoop() {\n",
        "120": "    addEdge(E11, N1, N1);\n",
        "121": "    assertEquals(1, graph.degree(N1));\n",
        "122": "    addEdge(E12, N1, N2);\n",
        "123": "    assertEquals(2, graph.degree(N1));\n",
        "124": "  }\n",
        "125": "\n",
        "126": "  @Test\n",
        "127": "  public void inDegree_selfLoop() {\n",
        "128": "    addEdge(E11, N1, N1);\n",
        "129": "    assertEquals(1, graph.inDegree(N1));\n",
        "130": "    addEdge(E41, N4, N1);\n",
        "131": "    assertEquals(2, graph.inDegree(N1));\n",
        "132": "  }\n",
        "133": "\n",
        "134": "  @Test\n",
        "135": "  public void outDegree_selfLoop() {\n",
        "136": "    addEdge(E11, N1, N1);\n",
        "137": "    assertEquals(1, graph.outDegree(N1));\n",
        "138": "    addEdge(E12, N1, N2);\n",
        "139": "    assertEquals(2, graph.outDegree(N1));\n",
        "140": "  }\n",
        "141": "\n",
        "142": "  @Test\n",
        "143": "  public void source_selfLoop() {\n",
        "144": "    addEdge(E11, N1, N1);\n",
        "145": "    assertEquals(N1, graph.source(E11));\n",
        "146": "  }\n",
        "147": "\n",
        "148": "  @Test\n",
        "149": "  public void target_selfLoop() {\n",
        "150": "    addEdge(E11, N1, N1);\n",
        "151": "    assertEquals(N1, graph.target(E11));\n",
        "152": "  }\n",
        "153": "\n",
        "154": "  @Override\n",
        "155": "  @Test\n",
        "156": "  public void addEdge_selfLoop() {\n",
        "157": "    assertTrue(addEdge(E11, N1, N1));\n",
        "158": "    assertThat(graph.edges()).contains(E11);\n",
        "159": "    assertThat(graph.edgesConnecting(N1, N1)).containsExactly(E11);\n",
        "160": "  }\n",
        "161": "\n",
        "162": "  @Test\n",
        "163": "  public void addEdge_existingSelfLoopEdgeBetweenSameNodes() {\n",
        "164": "    addEdge(E11, N1, N1);\n",
        "165": "    ImmutableSet<String> edges = ImmutableSet.copyOf(graph.edges());\n",
        "166": "    assertFalse(addEdge(E11, N1, N1));\n",
        "167": "    assertThat(graph.edges()).containsExactlyElementsIn(edges);\n",
        "168": "  }\n",
        "169": "\n",
        "170": "  @Test\n",
        "171": "  public void addEdge_existingEdgeBetweenDifferentNodes_selfLoops() {\n",
        "172": "    addEdge(E11, N1, N1);\n",
        "173": "    try {\n",
        "174": "      addEdge(E11, N1, N2);\n",
        "175": "      fail(\"Reusing an existing self-loop edge to connect different nodes succeeded\");\n",
        "176": "    } catch (IllegalArgumentException e) {\n",
        "177": "      assertThat(e.getMessage()).contains(ERROR_REUSE_EDGE);\n",
        "178": "    }\n",
        "179": "    try {\n",
        "180": "      addEdge(E11, N2, N2);\n",
        "181": "      fail(\"Reusing an existing self-loop edge to make a different self-loop edge succeeded\");\n",
        "182": "    } catch (IllegalArgumentException e) {\n",
        "183": "      assertThat(e.getMessage()).contains(ERROR_REUSE_EDGE);\n",
        "184": "    }\n",
        "185": "    addEdge(E12, N1, N2);\n",
        "186": "    try {\n",
        "187": "      addEdge(E12, N1, N1);\n",
        "188": "      fail(\"Reusing an existing edge to add a self-loop edge between different nodes succeeded\");\n",
        "189": "    } catch (IllegalArgumentException e) {\n",
        "190": "      assertThat(e.getMessage()).contains(ERROR_REUSE_EDGE);\n",
        "191": "    }\n",
        "192": "  }\n",
        "193": "\n",
        "194": "  @Test\n",
        "195": "  public void addEdge_parallelSelfLoopEdge() {\n",
        "196": "    addEdge(E11, N1, N1);\n",
        "197": "    try {\n",
        "198": "      addEdge(EDGE_NOT_IN_GRAPH, N1, N1);\n",
        "199": "      fail(\"Adding a parallel self-loop edge succeeded\");\n",
        "200": "    } catch (IllegalArgumentException e) {\n",
        "201": "      assertThat(e.getMessage()).contains(ERROR_PARALLEL_EDGE);\n",
        "202": "    }\n",
        "203": "  }\n",
        "204": "\n",
        "205": "  @Test\n",
        "206": "  public void removeNode_existingNodeWithSelfLoopEdge() {\n",
        "207": "    addNode(N1);\n",
        "208": "    addEdge(E11, N1, N1);\n",
        "209": "    assertTrue(graph.removeNode(N1));\n",
        "210": "    assertThat(graph.nodes()).isEmpty();\n",
        "211": "    assertThat(graph.edges()).doesNotContain(E11);\n",
        "212": "  }\n",
        "213": "\n",
        "214": "  @Test\n",
        "215": "  public void removeEdge_existingSelfLoopEdge() {\n",
        "216": "    addEdge(E11, N1, N1);\n",
        "217": "    assertTrue(graph.removeEdge(E11));\n",
        "218": "    assertThat(graph.edges()).doesNotContain(E11);\n",
        "219": "    assertThat(graph.edgesConnecting(N1, N1)).isEmpty();\n",
        "220": "  }\n",
        "221": "\n",
        "222": "  // TODO(kak): Can't we ditch this and just use PackageSanityTests?\n",
        "223": "  @Test\n",
        "224": "  public void testEquals() {\n",
        "225": "    MutableNetwork<Integer, String> graphA = createGraph();\n",
        "226": "    graphA.addNode(N1);\n",
        "227": "    MutableNetwork<Integer, String> graphB = createGraph();\n",
        "228": "    graphA.addNode(N2);\n",
        "229": "\n",
        "230": "    new EqualsTester()\n",
        "231": "        .addEqualityGroup(graphA)\n",
        "232": "        .addEqualityGroup(graphB)\n",
        "233": "        .testEquals();\n",
        "234": "  }\n",
        "235": "}\n"
    },
