        "451": "  public void testInverse() {\n",
        "452": "    assertEquals(\n",
        "453": "        ImmutableListMultimap.<Integer, String>of(),\n",
        "454": "        ImmutableListMultimap.<String, Integer>of().inverse());\n",
        "455": "    assertEquals(\n",
        "456": "        ImmutableListMultimap.of(1, \"one\"),\n",
        "457": "        ImmutableListMultimap.of(\"one\", 1).inverse());\n",
        "458": "    assertEquals(\n",
        "459": "        ImmutableListMultimap.of(1, \"one\", 2, \"two\"),\n",
        "460": "        ImmutableListMultimap.of(\"one\", 1, \"two\", 2).inverse());\n",
        "461": "    assertEquals(\n",
        "462": "        ImmutableListMultimap.of(\"of\", 'o', \"of\", 'f', \"to\", 't', \"to\", 'o').inverse(),\n",
        "463": "        ImmutableListMultimap.of('o', \"of\", 'f', \"of\", 't', \"to\", 'o', \"to\"));\n",
        "464": "    assertEquals(\n",
        "465": "        ImmutableListMultimap.of('f', \"foo\", 'o', \"foo\", 'o', \"foo\"),\n",
        "466": "        ImmutableListMultimap.of(\"foo\", 'f', \"foo\", 'o', \"foo\", 'o').inverse());\n",
        "467": "  }\n",
        "468": "\n",
        "469": "  public void testInverseMinimizesWork() {\n",
        "470": "    ImmutableListMultimap<String, Character> multimap =\n",
        "471": "        ImmutableListMultimap.<String, Character>builder()\n",
        "472": "            .put(\"foo\", 'f')\n",
        "473": "            .put(\"foo\", 'o')\n",
        "474": "            .put(\"foo\", 'o')\n",
        "475": "            .put(\"poo\", 'p')\n",
        "476": "            .put(\"poo\", 'o')\n",
        "477": "            .put(\"poo\", 'o')\n",
        "478": "            .build();\n",
        "479": "    assertSame(multimap.inverse(), multimap.inverse());\n",
        "480": "    assertSame(multimap, multimap.inverse().inverse());\n",
        "481": "  }\n",
        "482": "\n",
        "483": "  private static <K, V> void assertMultimapEquals(Multimap<K, V> multimap,\n",
        "484": "      Object... alternatingKeysAndValues) {\n",
        "485": "    assertEquals(multimap.size(), alternatingKeysAndValues.length / 2);\n",
        "486": "    int i = 0;\n",
        "487": "    for (Entry<K, V> entry : multimap.entries()) {\n",
        "488": "      assertEquals(alternatingKeysAndValues[i++], entry.getKey());\n",
        "489": "      assertEquals(alternatingKeysAndValues[i++], entry.getValue());\n",
        "490": "    }\n",
        "491": "  }\n",
        "492": "}\n",
        "493": "\n"
    }
