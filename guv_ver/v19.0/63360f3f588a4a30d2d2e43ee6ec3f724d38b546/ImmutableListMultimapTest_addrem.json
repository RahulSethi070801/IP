        "238": "  public void testBuilderOrderKeysBy() {\n",
        "239": "    ImmutableListMultimap.Builder<String, Integer> builder\n",
        "240": "        = ImmutableListMultimap.builder();\n",
        "241": "    builder.put(\"b\", 3);\n",
        "242": "    builder.put(\"d\", 2);\n",
        "243": "    builder.put(\"a\", 5);\n",
        "244": "    builder.orderKeysBy(Collections.reverseOrder());\n",
        "245": "    builder.put(\"c\", 4);\n",
        "246": "    builder.put(\"a\", 2);\n",
        "247": "    builder.put(\"b\", 6);\n",
        "248": "    ImmutableListMultimap<String, Integer> multimap = builder.build();\n",
        "249": "    assertThat(multimap.keySet()).containsExactly(\"d\", \"c\", \"b\", \"a\").inOrder();\n",
        "250": "    assertThat(multimap.values()).containsExactly(2, 4, 3, 6, 5, 2).inOrder();\n",
        "251": "    assertThat(multimap.get(\"a\")).containsExactly(5, 2).inOrder();\n",
        "252": "    assertThat(multimap.get(\"b\")).containsExactly(3, 6).inOrder();\n",
        "253": "  }\n",
