        "385": "\n",
        "386": "  private Multimap<String, Integer> createMultimap() {\n",
        "387": "    return ImmutableListMultimap.<String, Integer>builder()\n",
        "388": "        .put(\"foo\", 1).put(\"bar\", 2).put(\"foo\", 3).build();\n",
        "389": "  }\n",
        "390": "\n",
        "391": "  public void testMultimapReads() {\n",
        "392": "    Multimap<String, Integer> multimap = createMultimap();\n",
        "393": "    assertTrue(multimap.containsKey(\"foo\"));\n",
        "394": "    assertFalse(multimap.containsKey(\"cat\"));\n",
        "395": "    assertTrue(multimap.containsValue(1));\n",
        "396": "    assertFalse(multimap.containsValue(5));\n",
        "397": "    assertTrue(multimap.containsEntry(\"foo\", 1));\n",
        "398": "    assertFalse(multimap.containsEntry(\"cat\", 1));\n",
        "399": "    assertFalse(multimap.containsEntry(\"foo\", 5));\n",
        "400": "    assertFalse(multimap.entries().isEmpty());\n",
        "401": "    assertEquals(3, multimap.size());\n",
        "402": "    assertFalse(multimap.isEmpty());\n",
        "403": "    assertEquals(\"{foo=[1, 3], bar=[2]}\", multimap.toString());\n",
        "404": "  }\n",
